{
  "name": "metadator",
  "version": "1.0.0",
  "description": "Automatically generate metadata for Obsidian notes using Claude AI",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "bun run build.ts --watch",
    "build": "bun run check && bun run build.ts",
    "check": "biome check . && markdownlint '**/*.md'",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "validate": "bun run scripts/validate-plugin.ts",
    "version": "bun run version-bump.ts",
    "prepublishOnly": "bun run validate"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "metadata",
    "anthropic",
    "claude"
  ],
  "author": "Mark Ayers",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/bun": "^1.3.6",
    "@types/node": "25.0.8",
    "markdownlint-cli": "0.47.0",
    "obsidian": "1.11.4",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2"
  }
}
